{
    "name": "werkspot/message-queue",
    "type": "library",
    "description": "A generic message queue.",
    "keywords": ["message", "queue", "rabbitmq"],
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Werkspot Team",
            "email": "technology@werkspot.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Werkspot\\MessageQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Werkspot\\MessageQueue\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "php": "^7.1",
        "php-amqplib/php-amqplib": "^2.6",
        "psr/simple-cache": "^1.0",
        "ramsey/uuid": "^3.5",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "mockery/mockery": "v1.0.0-alpha1",
        "phpunit/phpunit": "^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
